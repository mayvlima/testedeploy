create table cliente( 
codigo                  number not null,
nome varchar(250)        not null,
cpf varchar(14)         not null,
telefone varchar(14)    not null,
email varchar(100)       not null,
logradouro varchar(250) not null,
numero varchar(15)      not null,
bairro  varchar(100)     not null,
complemento varchar(50)  not null,
cidade varchar(100)  not null,
estado varchar(50)  not null,
cep varchar(10)  not null);


insert into cliente 
(codigo, nome, cpf, telefone, email, logradouro, numero, bairro, complemento, cidade, estado, cep)
values (6, 'PEDRO', 217966709, 1190203362,'pedro@gmail.com','rua Augusto', 78, 'Parque da flores', 'casa', 'Belo Horizonte', 'Minas Gerais', 04567000);

