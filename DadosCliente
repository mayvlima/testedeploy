create table produto (codigo number(10), nome varchar(100),
valor_unitario double not null, quantidade_estoque int not null);

insert into produto
(codigo, nome, valor_unitario, quantidade_estoque)
values(1, 'Celular', 799.90, 6);

insert into produto
(codigo, nome, valor_unitario, quantidade_estoque)
values(2, 'Fritadeira', 249.67, 7);

insert into produto
(codigo, nome, valor_unitario, quantidade_estoque)
values(5, 'escova', 179.90, 4);

insert into produto
(codigo, nome, valor_unitario, quantidade_estoque)
values(9, 'Secador', 909.90, 9);

select * from produto;
